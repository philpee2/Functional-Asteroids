{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import",
        "babel"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 7
    },
    "globals": {
      "document": 1,
      "CanvasRenderingContext2D": 1,
      "confirm": 1
    },
    "rules": {
      "react/sort-comp": 0,
      "no-alert": 0,
      "arrow-parens": 0,
      "no-use-before-define": ["error", { "functions": false, "classes": false }]
    }
}
